{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "pty:default",
    "notification:default",
    "shell:default",
    "store:default",
    "stronghold:default",
    "websocket:default",
    "dialog:default",
    "shell:default",
    "fs:read-all",
    "fs:write-all",
    "shell:allow-execute",
    "shell:allow-spawn",
    "shell:allow-kill",
    "shell:allow-open",
    "shell:allow-stdin-write"
  ]
}
